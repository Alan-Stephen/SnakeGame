<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="560.0" prefWidth="870.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.snakeproject.Controllers.LeaderBoardController">
   <children>
      <ImageView fitHeight="560.0" fitWidth="870.0" pickOnBounds="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <image>
            <Image url="@../leaderBoardsBackGround.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="leaderBoardsTable" layoutX="270.0" layoutY="5.0" prefHeight="551.0" prefWidth="331.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="230.0" text="NAME" />
            <TableColumn fx:id="score" prefWidth="100.0" text="SCORE" />
        </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="&lt;- Back">
         <font>
            <Font name="DejaVu Serif Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
